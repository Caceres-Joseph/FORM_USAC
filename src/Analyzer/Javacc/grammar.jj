        /*
        +-------------------------+
        |   LST_P2
        */

nodeModel LST_P2(nodeModel nodo,tree arbol):
{
    nodeFila child;
}
{
    child=P2(arbol)
    {nodo.insertChildren(child);}

    LST_P2(nodo, arbol) 
    
    {return nodo;}
    | 
    epsilon()
 {return nodo;}
}

        /*
        +-------------------------+
        |   P2
        */

nodeFila P2(tree arbol):
{
    nodeFila nodRetorno=new nodeFila(arbol.tablaSimbolos);
}
{
    try{
         
        <tIdentificador> 
 
        <tDosPuntos>
        <tAbreCorchete>

        LST_ATRIBUTOS(nodRetorno.atrib)
        
        <tCierraCorchete> 

    }catch(ParseException e){
        this.error(e, arbol);
    }

    {return nodRetorno;}

}

